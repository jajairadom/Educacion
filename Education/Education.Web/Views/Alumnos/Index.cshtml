@model List<Education.BL.Alumno>

@{
    ViewBag.Title = "Index";
}

<h2>Alumnos</h2>

<table class="table table-bordered table-hover">
   <thead>
       <tr>
           <th>
               Codigo
           </th>
           <th>
               Descripcion
           </th>
           <th>
               RNE
           </th>
       </tr>
   </thead>
     @foreach (var alumno in Model)
    {
        <tr>
            <td>@alumno.Id</td>
            <td>@alumno.Descripcion</td>
            <td>@alumno.RNE</td>
        </tr>
    }
</table>
